        "from pgmpy.inference import VariableElimination"
      ],
      "metadata": {
        "id": "2pgN8twY1qjK"
      },
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "model=BayesianModel([('age','target'),('sex','target'),('exang','target'),('cp','target'),('target','restecg'),('target','chol')])\n",
        "model.fit(a,estimator=MaximumLikelihoodEstimator)\n",
        "test_inf=VariableElimination(model)\n",
        "q1=test_inf.query(variables=['target'],evidence={'restecg':1})\n",
        "print(q1)\n",
        "q2=test_inf.query(variables=['target'],evidence={'cp':2})\n",
        "print(q2)"
